#!/bin/bash

t0=$(sensors | awk '/Core 0/ {print $3}' | sed -e 's/\+//g' -e 's/째C//g' -e 's/\..*//g')
t1=$(sensors | awk '/Core 1/ {print $3}' | sed -e 's/\+//g' -e 's/째C//g' -e 's/\..*//g')
t2=$(sensors | awk '/Core 2/ {print $3}' | sed -e 's/\+//g' -e 's/째C//g' -e 's/\..*//g')
t3=$(sensors | awk '/Core 3/ {print $3}' | sed -e 's/\+//g' -e 's/째C//g' -e 's/\..*//g')

echo "$t0 $t1 $t2 $t3"

